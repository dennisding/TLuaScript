
import('struct')

class('CppLuaChannel', struct.cstruct)

function CppLuaChannel._init(self)
    print(_text('CppLuaChannel._init'))
end

function CppLuaChannel.say_hello(self, msg)
    print(_text('CppLuaChannel.say_hello'), msg)
end

function CppLuaChannel.say_hi(self)
    print(_text('CppLuaChannel.say_hi'))
end
