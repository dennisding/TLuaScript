
import('component')

class('CapsuleComponent', component.component)

function CapsuleComponent:InitCapsuleSize(radius, half_height)
	self.CapsuleRadius = radius
	self.CapsuleHalfHeight = half_height
end

function CapsuleComponent:init_size(radius, half_height)
	self.CapsuleRadius = radius
	self.CapsuleHalfHeight = half_height
end