

class('FVector')

function FVector:_init()
	print(_text('fvector init'))
end

function FVector.__add(a, b)
	local r = ctype.clone_type_info(a)
	r.x = a.x + b.x
	r.y = a.y + b.y
	r.z = a.z + b.z
	return r
end

function FVector:__tostring()
	return string.format('{x = %d, y = %d, z = %d}', self.x, self.y, self.z)
end